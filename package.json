{
  "name": "ngx-weui",
  "version": "5.0.0-alpha.1",
  "main": "./bundles/weui.umd.js",
  "module": "./esm5/weui.js",
  "es2015": "./esm2015/weui.js",
  "typings": "./weui.d.ts",
  "description": "weui for angular",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cipchk/ngx-weui.git"
  },
  "keywords": [
    "ngx-weui",
    "ng2-weui",
    "ngx weui",
    "ng2 weui",
    "angular weui",
    "angular2 weui",
    "angular4 weui",
    "weui"
  ],
  "author": "cipchk <cipchk>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cipchk/ngx-weui/issues"
  },
  "homepage": "https://github.com/cipchk/ngx-weui#readme",
  "scripts": {
    "analyze": "ng build --prod --build-optimizer --stats-json",
    "lint": "npm run lint:ts && npm run lint:style",
    "lint:ts": "tslint -p tsconfig.json -c tslint.json 'components/**/*.ts'",
    "lint:style": "stylelint \"{components}/**/*.less\" --syntax less",
    "tslint-check": "tslint-config-prettier-check ./tslint.json",
    "test": "ng test --code-coverage",
    "site:gen": "node scripts/site/main",
    "site:init": "node scripts/site/main init",
    "site:serve": "ng serve --port 0 --open",
    "site:build": "npm run site:init && ng build --prod --build-optimizer --base-href /ngx-weui/",
    "site:gh": "npm run site:build && gh-pages -d ./dist",
    "build": "bash ./build.sh",
    "release:next": "bash ./build.sh && cd publish && npm publish --access public --tag next",
    "release": "bash ./build.sh && cd publish && npm publish --access public"
  },
  "dependencies": {
    "@angular/animations": "^6.0.0",
    "@angular/common": "^6.0.0",
    "@angular/compiler": "^6.0.0",
    "@angular/core": "^6.0.0",
    "@angular/forms": "^6.0.0",
    "@angular/http": "^6.0.0",
    "@angular/platform-browser": "^6.0.0",
    "@angular/platform-browser-dynamic": "^6.0.0",
    "@angular/router": "^6.0.0",
    "angular-qq-maps": "^2.0.0",
    "core-js": "^2.5.4",
    "font-awesome": "^4.7.0",
    "ngx-countdown": "^3.0.0",
    "ngx-gesture-password": "^2.0.1",
    "ngx-notify": "^2.0.0",
    "rxjs": "^6.0.0",
    "swiper": "^4.2.6",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@schematics/angular": "^0.6.0",
    "@angular-devkit/build-angular": "~0.6.0",
    "@angular-devkit/schematics": "^0.6.0",
    "@angular/cli": "~6.0.0",
    "@angular/compiler-cli": "^6.0.0",
    "@angular/language-service": "^6.0.0",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "@types/sinon": "^2.3.1",
    "codecov": "^3.0.0",
    "codelyzer": "~4.2.1",
    "fs-extra": "^6.0.0",
    "g2-mobile": "^2.2.0-beta.3",
    "husky": "^0.14.3",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~1.7.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~1.4.2",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "lint-staged": "^7.0.5",
    "mark-twain": "^2.0.2",
    "mustache": "^2.3.0",
    "protractor": "~5.3.0",
    "rollup": "^0.49.2",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-sourcemaps": "^0.4.2",
    "rollup-plugin-uglify": "^2.0.1",
    "sinon": "^4.4.2",
    "stylelint": "^8.2.0",
    "stylelint-config-standard": "^18.0.0",
    "ts-node": "~5.0.1",
    "tslint": "~5.9.1",
    "typescript": "~2.7.2",
    "weui": "^1.1.2"
  },
  "lint-staged": {
    "*.{cmd,html,json,md,sh,txt,xml,yml}": [
      "editorconfig-tools fix",
      "git add"
    ],
    "components/**/*.ts": [
      "npm run lint:ts",
      "git add"
    ],
    "components/**/*.less": [
      "npm run lint:style",
      "git add"
    ]
  }
}
